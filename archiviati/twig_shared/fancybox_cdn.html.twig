{#

{%- include '@includes/fancybox_cdn.html.twig' with { target: 'head' | 'js' } -%}

#}

{% set js = 'https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.5.7/jquery.fancybox.min.js' %}
{% set css = 'https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.5.7/jquery.fancybox.min.css' %}

{%- if target == 'head' -%}
<link rel="preconnect" href="https://cdnjs.cloudflare.com">
<link rel="preload" href="{{ js }}" as="script">
<link rel="preload" href="{{ css }}" as="style">
<link rel="stylesheet" href="{{ css }}">

{%- else -%}
  <script src="{{ js }}"></script>
{%- endif -%}
